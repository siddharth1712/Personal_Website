{
    "functions": {
      "src/pages/api/*.js": {
        "runtime": "@vercel/node"
      }
    },
    "rewrites": [
      {
        "source": "/(.*).(pdf|png|jpg|jpeg|gif|svg|ico|css|js|woff2|woff|ttf|eot)",
        "destination": "/$1.$2"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/Siddharth_Resume.pdf",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/pdf"
          }
        ]
      }
    ]
  }
  
