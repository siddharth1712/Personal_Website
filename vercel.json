{
    "functions": {
      "src/pages/api/*.js": {
        "runtime": "@vercel/node"
      }
    },
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "/((?!.*\\.(?:pdf|png|jpg|jpeg|gif|svg|ico|css|js|woff2|woff|ttf|eot)).*)",
        "destination": "/index.html"
      }
    ]
  }
  